{
  "name": "react-test",
  "version": "1.0.0",
  "main": "index.jsx",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ViktoriiaKR/Posts-catalog-react-redux-"
  },
  "scripts": {
    "build": "yarn clean; webpack --config config/webpack-prod-config.js",
    "clean": "rm -rf build/*",
    "check-staged": "lint-staged",
    "lint": "eslint src -c .eslintrc.json --ext js,jsx",
    "sass-lint": "sass-lint -vq src/**/*.scss",
    "start:dev": "node config/webpack-dev-server.js --open",
    "start:prod": "yarn build; serve build"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^1.0.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.2",
    "node-sass": "^4.9.3",
    "path": "^0.12.7",
    "prettier": "^1.14.2",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.1.0",
    "serve": "^10.0.0",
    "style-loader": "^0.23.0",
    "webpack-command": "^0.4.1"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-hot-loader": "^4.3.6",
    "react-redux": "^7.2.3",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.3.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "uuid": "^8.3.2",
    "webpack": "^4.17.2",
    "webpack-dev-server": "^3.1.8",
    "webpack-merge": "^4.1.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint"
    ],
    "*.{js,jsx,json,md}": [
      "prettier --list-different"
    ],
    "src/**/*.scss": [
      "sass-lint -vq"
    ]
  }
}
